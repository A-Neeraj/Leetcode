class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> indices;
        for(int i=0;i<nums.size();i++)
        {
            vector<int>::iterator it=find(nums.begin()+i+1,nums.end(),target-nums[i]);
            if(it!=nums.end())
            {
                indices.push_back(i);
                indices.push_back(it-nums.begin());
                break;
            }
        }
        return indices;
    }
    
};
